<div class="parallax-container">
  <div class="parallax">
    <img src="<%= asset_path('indexbg3.jpg')%>" class="bg-image" />
  </div>
</div>
<div class="section white" id="selling">
  <div class="container">
    <h4>Imóveis a venda</h4>
    <div class="row">
      <a href="<%= sell_real_estate_index_path	%>" class="btn waves-effect waves-light black">Ver mais imóveis</a>
    </div>
    <div class="divider"></div>
    <div class="row">
      <% if @selling.any? %>
        <% @selling.each do |real_estate| %>
          <div class="col s4">
            <%= render partial: "shared/featuring_real_estate_card", locals: {real_estate: real_estate, renting: false, selling: true} %>
          </div>
        <% end %>
      <% else %>
        Nada a exibir agora!
      <% end %>
    </div>
  </div>
</div>